            "settings": {
              "image": "marvingarcia/cameracapture:2.1",
              "createOptions": "{\"Hostname\":\"cameracapture\",\"HostConfig\":{\"PortBindings\":{\"5000/tcp\":[{\"HostPort\":\"5000\"}]},\"Binds\":[\"/opt/vc:/opt/vc\"],\"Devices\":[{\"PathOnHost\":\"/dev/vchiq\",\"PathInContainer\":\"/dev/vchiq\",\"CgroupPermissions\":\"rwm\"},{\"PathOnHost\":\"/dev/vcsm-cma\",\"PathInContainer\":\"/dev/vcsm-cma\",\"CgroupPermissions\":\"rwm\"}]}}"
